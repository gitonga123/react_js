{
  "name": "react-boilerplate",
  "private": true,
  "version": "2.5.6",
  "description": "Boilerplate for [SurviveJS - React](http://survivejs.com/react/introduction/)",
  "scripts": {
    "stats": "webpack --profile --json > stats.json",
    "start": "webpack-dev-server",
    "deploy": "gh-pages -d build",
    "build": "webpack",
    "test": "karma start",
    "test:tdd": "npm run test -- --auto-watch --no-single-run",
    "test:lint": "eslint ./app ./tests --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-survivejs-kanban": "^0.3.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "eslint": "5.7.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "isparta-instrumenter-loader": "^1.0.1",
    "karma": "^3.1.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^3.0.5",
    "mocha": "^5.2.0",
    "npm-install-webpack-plugin": "4.0.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.22.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "webpack-validator": "^3.0.1"
